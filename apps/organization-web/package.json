{
  "name": "organization-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next dev -p 3002",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next build",
    "start": "next start -p 3002",
    "lint": "eslint",
    "lint:check": "eslint --quiet .",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format": "npm run format:check && npm run format:fix",
    "db:generate": "pnpm --filter @thrive/database run generate",
    "db:migrate": "pnpm --filter @thrive/database run migrate:dev",
    "db:push": "prisma db push",
    "db:studio": "pnpm --filter @thrive/database run studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "deploy:dev": "bash ../../deploy/scripts/deploy.sh --app organization-web --env dev",
    "deploy:staging": "bash ../../deploy/scripts/deploy.sh --app organization-web --env staging",
    "deploy:qa": "bash ../../deploy/scripts/deploy.sh --app organization-web --env qa",
    "deploy:production": "bash ../../deploy/scripts/deploy.sh --app organization-web --env production",
    "deploy:logs": "pm2 logs thrive-organization",
    "deploy:stop": "pm2 stop thrive-organization",
    "deploy:delete": "pm2 delete thrive-organization",
    "docker:build": "docker build -t organization-web:latest .",
    "docker:run": "docker build -t organization-web:latest . && docker run --name organization-web --env-file .env -d -p 3002:3002 organization-web:latest",
    "docker:stop": "docker stop organization-web",
    "docker:remove": "docker rm organization-web",
    "docker:dev:up": "cross-env NODE_ENV=development sh -c '(docker compose version > /dev/null 2>&1 && docker compose up --build -d) || docker-compose up --build -d'",
    "docker:dev:down": "cross-env NODE_ENV=development sh -c '(docker compose version > /dev/null 2>&1 && docker compose down -v) || docker-compose down -v'",
    "docker:staging:up": "cross-env NODE_ENV=staging sh -c '(docker compose version > /dev/null 2>&1 && docker compose -f docker-compose.staging.yml up --build -d) || docker-compose -f docker-compose.staging.yml up --build -d'",
    "docker:staging:down": "cross-env NODE_ENV=staging sh -c '(docker compose version > /dev/null 2>&1 && docker compose -f docker-compose.staging.yml down -v) || docker-compose -f docker-compose.staging.yml down -v'",
    "docker:prod:up": "cross-env NODE_ENV=production sh -c '(docker compose version > /dev/null 2>&1 && docker compose -f docker-compose.prod.yml up --build -d) || docker-compose -f docker-compose.prod.yml up --build -d'",
    "docker:prod:down": "cross-env NODE_ENV=production sh -c '(docker compose version > /dev/null 2>&1 && docker compose -f docker-compose.prod.yml down -v) || docker-compose -f docker-compose.prod.yml down -v'"
  },
  "optionalDependencies": {
    "@tailwindcss/oxide-linux-x64-gnu": "^4.0.1",
    "lightningcss-linux-x64-gnu": "^1.30.1"
  },
  "dependencies": {
    "@thrive/database": "workspace:*",
    "@auth/prisma-adapter": "^2.10.0",
    "@aws-sdk/client-dynamodb": "^3.933.0",
    "@aws-sdk/client-s3": "^3.896.0",
    "@aws-sdk/lib-dynamodb": "^3.933.0",
    "@aws-sdk/s3-request-presigner": "^3.896.0",
    "@googlemaps/js-api-loader": "^1.16.10",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/adapter-pg": "^7.2.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-table": "^8.21.3",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^10.1.0",
    "date-fns": "^4.1.0",
    "floating-ui": "^5.2.8",
    "formik": "^2.4.6",
    "googleapis": "^161.0.0",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.12.23",
    "lucide-react": "^0.539.0",
    "next": "^16.0.10",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.11",
    "pg": "^8.16.3",
    "pm2": "^6.0.11",
    "prisma": "^7.2.0",
    "react": "^19.2.3",
    "react-chartjs-2": "^5.3.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.62.0",
    "server-only": "^0.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "yup": "^1.7.0",
    "zod": "^4.1.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie": "^0.6.0",
    "@types/estree": "^1.0.8",
    "@types/google.maps": "^3.58.1",
    "@types/hoist-non-react-statics": "^3.3.7",
    "@types/json-schema": "^7.0.15",
    "@types/json5": "^0.0.30",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/ms": "^2.1.0",
    "@types/node": "^20.19.21",
    "@types/nodemailer": "^7.0.1",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "bash": "^0.0.1",
    "eslint": "^9",
    "eslint-config-next": "^15.5.9",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "tsx": "^4.20.4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.9.2"
  }
}
