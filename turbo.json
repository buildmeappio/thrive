{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build",
        "generate"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "generate": {
      "outputs": [
        "generated/**",
        "node_modules/.prisma/**"
      ]
    },
    "lint": {},
    "lint:check": {},
    "format:check": {},
    "format:fix": {}
  }
}