{
  "name": "admin-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:memory": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next build --turbopack",
    "start": "next start -p 3000",
    "lint": "eslint",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:test": "tsx scripts/test-database.ts",
    "db:pull": "node scripts/pull-schema.js && npx prisma generate",
    "predeploy": "chmod +x scripts/deploy.sh",
    "deploy:dev": "npm run predeploy && scripts/deploy.sh dev --app thrive-examiner --env .env.development --config ecosystem.config.js",
    "deploy:staging": "npm run predeploy && scripts/deploy.sh staging --app thrive-examiner --env .env.staging --config ecosystem.config.js",
    "deploy:qa": "npm run predeploy && scripts/deploy.sh qa --app thrive-examiner --env .env.qa --config ecosystem.config.js",
    "deploy:production": "npm run predeploy && scripts/deploy.sh production --app thrive-examiner --env .env.production --config ecosystem.config.js",
    "deploy:logs": "pm2 logs thrive-examiner",
    "deploy:stop": "pm2 stop thrive-examiner",
    "deploy:delete": "pm2 delete thrive-examiner",
    "docker:build": "docker build -t examiner:latest .",
    "docker:run": "docker build -t examiner:latest . && docker run -p 3001:3001 examiner:latest",
    "docker:stop": "docker stop examiner",
    "docker:remove": "docker rm examiner",
    "docker:dev:up": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 docker-compose up --build -d",
    "docker:dev:down": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 docker-compose down -v"
  },
  "optionalDependencies": {
    "@tailwindcss/oxide-linux-x64-gnu": "^4.0.1",
    "lightningcss-linux-x64-gnu": "^1.30.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.911.0",
    "@aws-sdk/s3-request-presigner": "^3.911.0",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-table": "^8.21.3",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "formik": "^2.4.6",
    "googleapis": "^159.0.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "libphonenumber-js": "^1.12.25",
    "lucide-react": "^0.542.0",
    "next": "^15.5.7",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.11",
    "pg": "^8.16.3",
    "prisma": "^7.1.0",
    "radix-ui": "^1.4.3",
    "react": "19.1.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "server-only": "^0.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^12.0.0",
    "yup": "^1.7.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.14",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}