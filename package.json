{
  "name": "thrive",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=16384 next dev",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=16384 next build",
    "start": "next start -p 3002",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "db:pull": "chmod +x scripts/pull-schema.sh && bash scripts/pull-schema.sh && npx prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "predeploy": "chmod +x scripts/deploy.sh",
    "deploy:dev": "npm run predeploy && scripts/deploy.sh dev --app thrive-organization --env .env.development --config ecosystem.config.js",
    "deploy:staging": "npm run predeploy && scripts/deploy.sh staging --app thrive-organization --env .env.staging --config ecosystem.config.js",
    "deploy:qa": "npm run predeploy && scripts/deploy.sh qa --app thrive-organization --env .env.qa --config ecosystem.config.js",
    "deploy:production": "npm run predeploy && scripts/deploy.sh production --app thrive-organization --env .env.production --config ecosystem.config.js",
    "deploy:logs": "pm2 logs thrive-organization",
    "deploy:stop": "pm2 stop thrive-organization",
    "deploy:delete": "pm2 delete thrive-organization",
    "docker:build": "docker build -t examiner:latest .",
    "docker:run": "docker build -t examiner:latest . && docker run --env-file .env -d -p 3002:3002 examiner:latest",
    "docker:stop": "docker stop examiner",
    "docker:remove": "docker rm examiner",
    "docker:up": "docker-compose up --build -d",
    "docker:down": "docker-compose down -v"
  },
  "optionalDependencies": {
    "@tailwindcss/oxide-linux-x64-gnu": "^4.0.1",
    "lightningcss-linux-x64-gnu": "^1.30.1"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@aws-sdk/client-dynamodb": "^3.933.0",
    "@aws-sdk/client-s3": "^3.896.0",
    "@aws-sdk/lib-dynamodb": "^3.933.0",
    "@aws-sdk/s3-request-presigner": "^3.896.0",
    "@casl/prisma": "^1.5.2",
    "@casl/react": "^5.0.0",
    "@googlemaps/js-api-loader": "^1.16.10",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-table": "^8.21.3",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^10.1.0",
    "date-fns": "^4.1.0",
    "floating-ui": "^5.2.8",
    "formik": "^2.4.6",
    "googleapis": "^161.0.0",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.12.23",
    "lucide-react": "^0.539.0",
    "next": "^15.5.4",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "pm2": "^6.0.11",
    "prisma": "^6.14.0",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "server-only": "^0.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "yup": "^1.7.0",
    "zod": "^4.0.17",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie": "^0.6.0",
    "@types/estree": "^1.0.8",
    "@types/google.maps": "^3.58.1",
    "@types/hoist-non-react-statics": "^3.3.7",
    "@types/json-schema": "^7.0.15",
    "@types/json5": "^0.0.30",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/ms": "^2.1.0",
    "@types/node": "^20.19.21",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "bash": "^0.0.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "tsx": "^4.20.4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
