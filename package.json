{
  "name": "prisma-db",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "generate": "prisma generate",
    "migrate:create": "prisma migrate dev --create-only",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "studio": "prisma generate && prisma studio",
    "reset": "prisma migrate reset",
    "seed": "prisma db seed",
    "docker:up": "docker-compose --env-file .env -f docker-compose.dev.yaml up -d",
    "docker:down": "docker-compose --env-file .env -f docker-compose.dev.yaml down -v",
    "db:sync": "chmod +x scripts/db-sync.sh && bash scripts/db-sync.sh"
  },
