{
  "name": "examiner-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:pull": "chmod +x scripts/pull-schema.sh && scripts/pull-schema.sh",
    "predeploy": "chmod +x scripts/deploy.sh",
    "deploy:dev": "npm run predeploy && scripts/deploy.sh dev --app thrive-examiner --env .env.development --config ecosystem.config.js",
    "deploy:staging": "npm run predeploy && scripts/deploy.sh staging --app thrive-examiner --env .env.staging --config ecosystem.config.js",
    "deploy:qa": "npm run predeploy && scripts/deploy.sh qa --app thrive-examiner --env .env.qa --config ecosystem.config.js",
    "deploy:production": "npm run predeploy && scripts/deploy.sh production --app thrive-examiner --env .env.production --config ecosystem.config.js",
    "deploy:logs": "pm2 logs thrive-examiner",
    "deploy:stop": "pm2 stop thrive-examiner",
    "deploy:delete": "pm2 delete thrive-examiner"
  },
  "dependencies": {
    "@prisma/client": "^6.16.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "formik": "^2.4.6",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
    "pm2": "^6.0.10",
    "prisma": "^6.16.1",
    "radix-ui": "^1.4.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "yup": "^1.7.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "typescript": "^5"
  }
}
