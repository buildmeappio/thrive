# Thrive Admin Web - Local Development
# Copy to apps/admin-web/.env or run: pnpm run setup:local

# Database (shared)
DATABASE_URL=postgresql://user:password@localhost:5432/thrive
DATABASE_SSL_REQUIRED=false

# App URLs
NEXT_PUBLIC_APP_URL=http://localhost:3000/admin
NEXT_PUBLIC_CDN_URL=
NEXT_PUBLIC_CLAIMANT_AVAILABILITY_URL=/claimant/availability
FRONTEND_URL=http://localhost:3000

# AWS (optional for local)
AWS_REGION=ca-central-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# JWT / Auth (generate secure values for local)
NEXTAUTH_SECRET=your-nextauth-secret

# Node
NODE_ENV=development
