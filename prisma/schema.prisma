  ACCEPTED
  REJECTED
}

enum UrgencyLevel {
  HIGH
  MEDIUM
  LOW
}

enum ExaminationSecureLinkStatus {
  PENDING
  SUBMITTED
  INVALID
}

enum ClaimantPreference {
  IN_PERSON
  VIRTUAL
  EITHER
}

enum TimeBand {
  MORNING
  AFTERNOON
  EVENING
  EITHER
}

enum ExaminerStatus {
  PENDING
  ACCEPTED
  REJECTED
  INFO_REQUESTED
}
