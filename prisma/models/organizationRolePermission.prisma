model OrganizationRolePermission {
  organizationRoleId String @map("organization_role_id") @db.Uuid
  permissionId       String @map("permission_id") @db.Uuid

  organizationRole OrganizationRole @relation(fields: [organizationRoleId], references: [id])
  permission       Permission       @relation(fields: [permissionId], references: [id])

  @@id([organizationRoleId, permissionId])
  @@index([permissionId])
  @@map("organization_role_permissions")
}
